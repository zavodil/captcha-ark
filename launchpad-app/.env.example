# React app port (for development only)
PORT=3974

# NEAR contract ID
REACT_APP_CONTRACT_ID=tokensale.testnet

# NEAR network (testnet or mainnet)
REACT_APP_NEAR_NETWORK=testnet

# Backend API base URL (for production)
# WebSocket will connect to the same domain (wss://api-launchpad.nearspace.info/ws)
REACT_APP_API_BASE_URL=https://api-launchpad.nearspace.info/api

# hCaptcha site key (optional, defaults to test key)
REACT_APP_HCAPTCHA_SITE_KEY=10000000-ffff-ffff-ffff-000000000001
